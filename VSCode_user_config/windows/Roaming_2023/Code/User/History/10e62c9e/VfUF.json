[
  {
    "key": "ctrl+alt+d",
    "command": "workbench.action.tasks.runTask",
    "args": "deploy"
  },
  {
    "key": "ctrl+alt+w",
    "command": "workbench.action.tasks.runTask",
    "args": "qmlscene6"
  },
  {
    "key": "ctrl+alt+q",
    "command": "workbench.action.tasks.runTask",
    "args": "qmlscene"
  },
  {
    "key": "ctrl+alt+r",
    "command": "workbench.action.tasks.runTask",
    "args": "run"
  },
  {
    "key": "ctrl+alt+2",
    "command": "workbench.action.tasks.runTask",
    "args": "python"
  },
  {
    "key": "ctrl+alt+3",
    "command": "workbench.action.tasks.runTask",
    "args": "python3"
  },
  {
    "key": "ctrl+alt+b",
    "command": "workbench.action.tasks.runTask",
    "args": "build"
  },
  {
    "key": "ctrl+alt+c",
    "command": "workbench.action.tasks.runTask",
    "args": "clean"
  },
  {
    "key": "ctrl+alt+n",
    "command": "workbench.action.tasks.runTask",
    "args": "build_debug"
  },
  {
    "key": "alt+shift+b",
    "command": "workbench.action.tasks.runTask",
    "args": "build_vs"
  },
  {
    "key": "ctrl+shift+q",
    "command": "workbench.action.tasks.runTask",
    "args": "qtcreator"
  },
  {
    "key": "ctrl+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "ctrl+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "key": "ctrl+t",
    "command": "editor.action.transformToTitlecase"
  },
  {
    "key": "ctrl+]",
    "command": "-editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+]",
    "command": "-markdown.extension.onIndentLines",
    "when": "editorTextFocus && !suggestWidgetVisible && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "ctrl+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "ctrl+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "ctrl+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "ctrl+f7",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "ctrl+shift+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+down",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+=",
    "command": "editor.foldAll"
  },
  {
    "key": "ctrl+alt+-",
    "command": "editor.unfoldAll"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.tasks.runTask",
    "args": "format_qml",
    "when": "editorTextFocus && !editorReadonly && resourceExtname == .qml"
  },
  {
    "key": "shift+alt+f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
    "key": "ctrl+shift+a",
    "command": "workbench.view.extension.codemap-explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "ctrl+shift+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  { 
    "key": "ctrl+shift+s", 
    "command": "workbench.action.files.saveAll" 
  },
  
  { 
    "key": "ctrl+alt+shift+r", 
    "command": "workbench.action.reopenWithEditor" 
  },
  
]