{
	"name": "theme-madoodia-vscode",
	"displayName": "madoodia",
	"description": "Professional theme and matching icons, from the author of the original madoodia color scheme.",
	"publisher": "madoodia",
	"version": "1.1.14",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"commands": [
			{
				"command": "madoodia.enter_license",
				"title": "madoodia: enter license"
			},
			{
				"command": "madoodia.select_theme",
				"title": "madoodia: select theme"
			},
			{
				"command": "madoodia.activate_icons",
				"title": "madoodia: activate icons"
			}
		],
		"themes": [
			{
				"label": "madoodia",
				"uiTheme": "vs-dark",
				"path": "./themes/madoodia.json"
			}
		],
		"iconThemes": [],
		"configuration": {
			"type": "object",
			"title": "madoodia Settings",
			"properties": {
				"madoodiaPro.fileIconsMonochrome": {
					"type": "boolean",
					"default": false,
					"description": "makes the file icons monochrome"
				}
			}
		}
	},
	"devDependencies": {
		"vscode": "^1.0.0"
	},
	"extensionKind": "ui",
	"homepage": "https://madoodia",
	"bugs": {
		"url": "https://github.com/madoodia/madoodia-vscode"
	},
	"icon": "icon.png",
	"keywords": [
		"beautiful functionality",
		"focus",
		"minimal",
		"icons",
		"syntax highlighting"
	],
	"main": "./js/main",
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"*"
	],
	"qna": false,
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"__metadata": {
		"id": "f5d7ffda-c1d6-4070-ba80-803c705a1ee6",
		"publisherId": "f1aa9b60-e034-4838-a0dc-4a6a074cf48e",
		"publisherDisplayName": "madoodia"
	}
}