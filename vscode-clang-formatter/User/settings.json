{
    "sublimeTextKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "window.zoomLevel": 0,
    "editor.fontSize": 13,
    "git.confirmSync": false,
    "material-icon-theme.showWelcomeMessage": false,
    "files.associations": {
        "*.nmake": "makefile",
        "*.gnu": "makefile",
        "*.win": "makefile",
        "*.inc": "makefile"
    },
    "explorer.confirmDelete": false,
    "terminal.integrated.shellArgs.windows": [
        "-ExecutionPolicy",
        "Bypass"
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "files.autoSave": "onFocusChange",
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "C:/Users/madoodia/AppData/Roaming/Python/Python37/Scripts/black.exe",
    "editor.formatOnType": true,
    "python.formatting.autopep8Path": "black",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.intelliSenseCachePath": "D:/vscode_cache",
    "git.enableSmartCommit": true,
    "C_Cpp.default.cppStandard": "c++11",
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_path": "${SDKS_LOCATION}/libclang/bin/clang-format.exe",
}